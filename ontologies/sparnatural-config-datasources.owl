        <sparnatural-config-datasources:queryString>SELECT ?uri ?count (CONCAT(STR(?uri), &apos; (&apos;, STR(?count), &apos;)&apos;) AS ?label)
WHERE {
{
  SELECT DISTINCT ?uri (COUNT(?domain) AS ?count)
  WHERE {
    ?domain a $domain .
