{
  "name": "sparnatural",
  "version": "11.0.0",
  "description": "Visual client-side SPARQL query builder and knowledge graph exploration tool",
  "main": "dist/js/index.js",
  "module": "dist/js/index.js",
  "types": "dist/types/index.d.ts",
  "browser": "dist/browser/sparnatural.js",
