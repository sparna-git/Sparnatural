###  https://sparna-git.github.io/sparnatural-rico-1.0/ontology#query_tree_children_langfr
:query_tree_children_langfr rdf:type owl:NamedIndividual ;
                            <http://data.sparna.fr/ontologies/sparnatural-config-datasources#queryString> """# Selects the children of a node
SELECT DISTINCT ?uri ?label ?hasChildren
WHERE {
  $node $childrenPath ?uri .
  ?uri $labelPath ?label .
  FILTER(isIRI(?uri))
  FILTER(lang(?label) = '' || lang(?label) = $lang || lang(?label) = 'fr')
  OPTIONAL {
  	?uri $childrenPath ?children .
  }
  BIND(IF(bound(?children),true,false) AS ?hasChildren)
}
ORDER BY UCASE(?label)""" .


